;---------------------------------------
;@ENTER_MY_HOTEL
;여관에 타겟이 방문하였을 때.
;CHARANUM = 등록된 캐릭터의 수
;#DIM SAVEDATA BRAVE_HERO_FLAG
;용사 출현 여부 플래그
;---------------------------------------
@ENTER_MY_HOTEL
#DIM DYNAMIC DICE
;랜덤돌려서 구한 값

;캐릭터가 많을 경우 생성 중단
IF CHARANUM >= 98
    PRINTW 캐릭터 수 초과 오류
    RETURN 0
ENDIF

PRINTW 조교하기 적당한 년이 들어왔다!
;-----네임드 캐릭터-----



;-----랜덤 캐릭터-----
;E = CHARANUM - 1 = 만들어진 캐릭터의 번호
IF DAY > 50 && 여관LV > 10 && RAND:20 == 0
;용사 등장
    ADDCHARA 11
    E = CHARANUM - 1
    CALL RAND_CHARA_MAKE, 11
ELSEIF RAND:20 == 0 && BRAVE_HERO_FLAG == 1
;성녀 등장
    ADDCHARA 12
    E = CHARANUM - 1
    CALL RAND_CHARA_MAKE, 12
ELSE
;일반 타겟 등장
    DICE = (RAND:6)+1
    ADDCHARA DICE
    E = CHARANUM - 1
    CALL RAND_CHARA_MAKE, DICE
ENDIF


;---------------------------------------
;@CHARA_MAKE, CHARA
;네임드 캐릭터 만들기
;---------------------------------------
@CHARA_MAKE, CHARA
#DIM DYNAMIC CHARA

ADDCHARA CHARA
IF FLAG:28 == 1
;네임드 강제처녀
    TALENT:(CHARANUM - 1):1 = 0
    TALENT:(CHARANUM - 1):2 = 0
ENDIF
;---------------------------------------
;@RAND_CHARA_MAKE, CHARA_JOB
;랜덤 캐릭터 만들기
;CALL GIVE_RAND_NUM, RAND_MIN, RAND_MAX
;주어진 최소 초과, 최대 이하에서 랜덤 값 구하기.
;#DIM SAVEDATA GAME_MODE_LV
;게임 난이도 LV
;#DIM SAVEDATA BRAVE_HERO_FLAG
;용사 출현 여부 플래그
;---------------------------------------
@RAND_CHARA_MAKE, CHARA_JOB
#DIM DYNAMIC CHARA_XX
;캐릭터 번호. CHARAXX.csv와 같음
#DIM DYNAMIC MAKE_CHARA_NUM
;만들어진 캐릭터의 번호
#DIM DYNAMIC DICE
;랜덤돌려서 구한 값
#DIM DYNAMIC CHARA_JOB
;캐릭터 전투 직업
MAKE_CHARA_NUM = E

$CHARA_REROLL

;---- 초기설정 ----
;****************************
CFLAG:MAKE_CHARA_NUM:5 = 1000
;시작 소지금 설정 -> 직업과 레벨에 따라 이후 변경
CFLAG:MAKE_CHARA_NUM:1 = 0
CFLAG:MAKE_CHARA_NUM:2 = 0
CFLAG:MAKE_CHARA_NUM:3 = 0
;호감도,신뢰도,욕구불만도
CFLAG:MAKE_CHARA_NUM:31 = 0
;관계 (BIT 0=낯선 1=지인 2=친한 3=연모 4=연인 5=아내)


IF RAND:3 == 0
	TALENT:MAKE_CHARA_NUM:4 = 0
ELSE
	TALENT:MAKE_CHARA_NUM:4 = 1
ENDIF
;키스경험(0=YES, 1=NO) 비처녀 키스경험YES로 됨.

DICE = (RAND:10)+1
IF FLAG:26 == 0
    TALENT:MAKE_CHARA_NUM:1 = 1
	TALENT:MAKE_CHARA_NUM:4 = 1
ELSEIF FLAG:26 <= DICE
    TALENT:MAKE_CHARA_NUM:1 = 0
	;처녀
ELSE
    TALENT:MAKE_CHARA_NUM:1 = 1
	TALENT:MAKE_CHARA_NUM:4 = 1
ENDIF
;처녀 상태(0=YES, 1=NO)

DICE = (RAND:10)+1
IF FLAG:27 == 0
    TALENT:MAKE_CHARA_NUM:2 = 1
	TALENT:MAKE_CHARA_NUM:4 = 1
ELSEIF FLAG:27 <= DICE
    TALENT:MAKE_CHARA_NUM:2 = 0
	;A처녀
ELSE
    TALENT:MAKE_CHARA_NUM:2 = 1
	TALENT:MAKE_CHARA_NUM:4 = 1
ENDIF
;A처녀 상태(0=YES, 1=NO)

IF TALENT:MAKE_CHARA_NUM:1 == 0 && RAND:10 != 0
	TALENT:MAKE_CHARA_NUM:2 = 0
ENDIF
;V처녀면 A처녀일 확률도 매우 높음.

CFLAG:MAKE_CHARA_NUM:341 = 0
CFLAG:MAKE_CHARA_NUM:342 = 0
CFLAG:MAKE_CHARA_NUM:343 = 0
CFLAG:MAKE_CHARA_NUM:344 = 0
;의뢰
TALENT:MAKE_CHARA_NUM:801 = 1
;계급 - 평민
TALENT:MAKE_CHARA_NUM:806 = 1
;경력 - 초보자
TALENT:MAKE_CHARA_NUM:601 = 1
;종족 - 인간
CFLAG:MAKE_CHARA_NUM:4 = 1
EXP:MAKE_CHARA_NUM:151 = 0
;직업레벨, 직업 경험치 설정
CFLAG:MAKE_CHARA_NUM:600 = (RAND:7)+1
;생리주기
;****************************

;---- 직업 확인 ----
SELECTCASE CHARA_JOB
    CASE 1
        CHARA_JOB = 701
    CASE 2
        CHARA_JOB = 702
    CASE 3
        CHARA_JOB = 703
    CASE 4
        CHARA_JOB = 704
    CASE 5
        CHARA_JOB = 705
    CASE 6
        CHARA_JOB = 706
    CASE 11
        CHARA_JOB = 752
    CASE 12
        CHARA_JOB = 753
ENDSELECT
;--------------------
TALENT:MAKE_CHARA_NUM:CHARA_JOB = 1
;직업 확정
CFLAG:MAKE_CHARA_NUM:13 = CHARA_JOB
;직업번호 기억하기.

;-----TALENT 설정-----
CALL SET_TALENT, MAKE_CHARA_NUM

;-----신의가호:5-----
IF RAND:20 == 0
    ;DICE = 1 + RAND:5
    ;CFLAG:MAKE_CHARA_NUM:7 = DICE
	CFLAG:MAKE_CHARA_NUM:7 = 1
ENDIF

IF CFLAG:MAKE_CHARA_NUM:7 != 0
    IF CFLAG:MAKE_CHARA_NUM:7 == 1
    ;순결의 가호
        TALENT:MAKE_CHARA_NUM:1 = 0
        ;처녀
		CFLAG:MAKE_CHARA_NUM:22 = 1
		;순결봉인 ON
    ENDIF
ENDIF

;-----계급 설정-----
DICE = RAND:30
IF DICE < 5 && TALENT:MAKE_CHARA_NUM:753 == 0 && TALENT:MAKE_CHARA_NUM:752 == 0
	TALENT:MAKE_CHARA_NUM:801 = 0
	;노예
ELSEIF DICE == 29
	IF RAND:20 == 0
		TALENT:MAKE_CHARA_NUM:801 = 3
		;왕족
	ELSE
		TALENT:MAKE_CHARA_NUM:801 = 2
		;귀족
	ENDIF
ENDIF

;-----경력 설정-----
IF DAY < 50
	DICE = RAND:3
	TALENT:MAKE_CHARA_NUM:806 = DICE+1
	;초보자 ~ E급 용병
ELSE
	IF 30 - (여관LV/10) < 10
		DICE = RAND:10
	ELSE
		DICE = RAND:(30-(여관LV/10))
	ENDIF

	IF DICE == 0
		TALENT:MAKE_CHARA_NUM:806 = 8
		;S급
	ELSEIF DICE >= 1 && DICE < 3
		TALENT:MAKE_CHARA_NUM:806 = 7
		;A급
	ELSEIF DICE >= 3 && DICE < 6
		TALENT:MAKE_CHARA_NUM:806 = 6
		;B급
	ELSE
		TALENT:MAKE_CHARA_NUM:806 = (DICE%5) + 1
		;늅~C급
	ENDIF
ENDIF

;-----직업LV 설정-----
;랜덤성을 위해 직업LV 한자리 수 더하기
DICE = (RAND:10)
IF DICE < 8
;0~5를 더함
    CFLAG:MAKE_CHARA_NUM:4 += RAND:6
ELSE
;6~10을 더함
    CALL GIVE_RAND_NUM, 5, 10
    CFLAG:MAKE_CHARA_NUM:4 += RESULT
ENDIF

;CFLAG:MAKE_CHARA_NUM:4 += GAME_MODE_LV * 10
;난이도 상승에 따른 타겟의 직업LV 상승

CFLAG:MAKE_CHARA_NUM:4 += 10 * ((TALENT:MAKE_CHARA_NUM:806)-1)
;경력에 따른 직업LV 상승

;-----소지금 설정-----
SIF TALENT:MAKE_CHARA_NUM:752 == 1 || TALENT:MAKE_CHARA_NUM:753 == 1
    CFLAG:MAKE_CHARA_NUM:5 += 20000
CFLAG:MAKE_CHARA_NUM:5 += ((CFLAG:MAKE_CHARA_NUM:4) - 1) * 1000
POWER LOCAL, 10, TALENT:MAKE_CHARA_NUM:806
;10의 X승을 LOCAL에 저장.
CFLAG:MAKE_CHARA_NUM:5 += RAND:LOCAL
;랜덤 값을 소지금에 추가.
CFLAG:MAKE_CHARA_NUM:5 += 2000 * ((TALENT:MAKE_CHARA_NUM:806)-1)
;경력에 따른 기본급 추가.

;-----레벨에 따른 능력치 올리기-----
IF CFLAG:MAKE_CHARA_NUM:4 > 1
    REPEAT (CFLAG:MAKE_CHARA_NUM:4) - 1
        CALL JOB_LEVEL_UP, MAKE_CHARA_NUM, CHARA_JOB
    REND
ENDIF

;-----체형 조정-----
CALL CHAR_SIZE_GENERATE, MAKE_CHARA_NUM

IF RESULT:0 < 20 && RAND:3 != 0
	RESULT:0 += RAND:10
ENDIF
;나이가 어리다면 일정확률로 상승.
REPEAT 6
	CFLAG:MAKE_CHARA_NUM:(COUNT+31) = RESULT:COUNT
REND
;체형 결과 값 대입.

;-----어색한 TALENT 수정-----
CALL SET_TALENT_2, MAKE_CHARA_NUM

;-----EXP설정-----
CALL SET_EXP, MAKE_CHARA_NUM

;-----ABL설정-----
CALL SET_ABL, MAKE_CHARA_NUM

;랜덤 네임 지정하기
CALL RANDOM_NAME, 0, MAKE_CHARA_NUM

;-----캐릭터 리롤묻기-----
CALL SHOW_TARGET_STATE, MAKE_CHARA_NUM, -1

WHILE 1
    IF RESULT == 1
    ;리롤!
        IF DAY > 50 && 여관LV > 10 && RAND:20 == 0
        ;용사OR성녀 일때
            DICE = RAND:2
            IF DICE == 0
                CHARA_JOB = 11
            ELSEIF DICE == 1 && BRAVE_HERO_FLAG == 1
                CHARA_JOB = 12
            ELSE
                CHARA_JOB = 11
            ENDIF
        ELSE
        ;일반 직업일 때
            CHARA_JOB = 1 + RAND:6
        ENDIF
        
        VARSET TALENT:MAKE_CHARA_NUM:0, 0
        ;TALENT 초기화
		VARSET CFLAG:MAKE_CHARA_NUM:0, 0
		;CFLAG 초기화
		VARSET ABL:MAKE_CHARA_NUM:0, 0
		;AVL 초기화
		VARSET EXP:MAKE_CHARA_NUM:0, 0
		;EXP 초기화
		VARSET PALAM:MAKE_CHARA_NUM:0, 0
		VARSET JUEL:MAKE_CHARA_NUM:0, 0
		;PALAM, JUEL 초기화
		VARSET BASE:MAKE_CHARA_NUM:0, 0
		VARSET MAXBASE:MAKE_CHARA_NUM:0, 0
		;BASE 초기화

		SELECTCASE CHARA_JOB
			CASE 1
			;전사
				MAXBASE:MAKE_CHARA_NUM:0 = 700
				BASE:MAKE_CHARA_NUM:0 = 700
				MAXBASE:MAKE_CHARA_NUM:1 = 300
				BASE:MAKE_CHARA_NUM:1 = 300
				ABL:MAKE_CHARA_NUM:91 = 15
				ABL:MAKE_CHARA_NUM:92 = 15
				ABL:MAKE_CHARA_NUM:93 = 5
				CFLAG:MAKE_CHARA_NUM:99 = 1
			
			CASE 2
			;마법사
				MAXBASE:MAKE_CHARA_NUM:0 = 300
				BASE:MAKE_CHARA_NUM:0 = 300
				MAXBASE:MAKE_CHARA_NUM:1 = 700
				BASE:MAKE_CHARA_NUM:1 = 700
				ABL:MAKE_CHARA_NUM:91 = 5
				ABL:MAKE_CHARA_NUM:92 = 5
				ABL:MAKE_CHARA_NUM:93 = 20
				CFLAG:MAKE_CHARA_NUM:99 = 1
			CASE 3
			;사제
				MAXBASE:MAKE_CHARA_NUM:0 = 300
				BASE:MAKE_CHARA_NUM:0 = 300
				MAXBASE:MAKE_CHARA_NUM:1 = 700
				BASE:MAKE_CHARA_NUM:1 = 700
				ABL:MAKE_CHARA_NUM:91 = 5
				ABL:MAKE_CHARA_NUM:92 = 5
				ABL:MAKE_CHARA_NUM:93 = 15
				CFLAG:MAKE_CHARA_NUM:99 = 1
			CASE 4
			;도적
				MAXBASE:MAKE_CHARA_NUM:0 = 500
				BASE:MAKE_CHARA_NUM:0 = 500
				MAXBASE:MAKE_CHARA_NUM:1 = 400
				BASE:MAKE_CHARA_NUM:1 = 400
				ABL:MAKE_CHARA_NUM:91 = 20
				ABL:MAKE_CHARA_NUM:92 = 5
				ABL:MAKE_CHARA_NUM:93 = 5
				CFLAG:MAKE_CHARA_NUM:99 = 1
			CASE 5
			;무녀
				MAXBASE:MAKE_CHARA_NUM:0 = 300
				BASE:MAKE_CHARA_NUM:0 = 300
				MAXBASE:MAKE_CHARA_NUM:1 = 700
				BASE:MAKE_CHARA_NUM:1 = 700
				ABL:MAKE_CHARA_NUM:91 = 10
				ABL:MAKE_CHARA_NUM:92 = 10
				ABL:MAKE_CHARA_NUM:93 = 15
				CFLAG:MAKE_CHARA_NUM:99 = 1
			CASE 6
			;궁수
				MAXBASE:MAKE_CHARA_NUM:0 = 400
				BASE:MAKE_CHARA_NUM:0 = 400
				MAXBASE:MAKE_CHARA_NUM:1 = 600
				BASE:MAKE_CHARA_NUM:1 = 600
				ABL:MAKE_CHARA_NUM:91 = 15
				ABL:MAKE_CHARA_NUM:92 = 5
				ABL:MAKE_CHARA_NUM:93 = 10
				CFLAG:MAKE_CHARA_NUM:99 = 1
			CASE 11
			;용사
				MAXBASE:MAKE_CHARA_NUM:0 = 1000
				BASE:MAKE_CHARA_NUM:0 = 1000
				MAXBASE:MAKE_CHARA_NUM:1 = 1000
				BASE:MAKE_CHARA_NUM:1 = 1000
				ABL:MAKE_CHARA_NUM:91 = 30
				ABL:MAKE_CHARA_NUM:92 = 30
				ABL:MAKE_CHARA_NUM:93 = 30
				CFLAG:MAKE_CHARA_NUM:99 = 1
			CASE 13
			;성녀
				MAXBASE:MAKE_CHARA_NUM:0 = 700
				BASE:MAKE_CHARA_NUM:0 = 700
				MAXBASE:MAKE_CHARA_NUM:1 = 1500
				BASE:MAKE_CHARA_NUM:1 = 1500
				ABL:MAKE_CHARA_NUM:91 = 15
				ABL:MAKE_CHARA_NUM:92 = 15
				ABL:MAKE_CHARA_NUM:93 = 45
				CFLAG:MAKE_CHARA_NUM:99 = 1
		ENDSELECT
		;초기화 된 직업의 기초 능력치 다시 부여.
		;직업 추가할 때 반드시 수정할 것. !수정JOB
        
        GOTO CHARA_REROLL
        BREAK
    ELSEIF RESULT == 0
    ;확정
        SIF TALENT:MAKE_CHARA_NUM:752 == 1
        ;용사출현 플래그
            BRAVE_HERO_FLAG = 1
        PRINTFORMW %조사처리(CALLNAME:MAKE_CHARA_NUM,"가")% 여관에 체크인 하였습니다!. 한동안 이곳에 머물것으로 보입니다.
        BREAK
    ELSEIF RESULT == 99
    ;내쫓기
        PRINTFORMW %조사처리(NAME:MAKE_CHARA_NUM,"는")% 씨근덕거리며 여관에서 나왔다.
        DELCHARA MAKE_CHARA_NUM
        BREAK
    ELSE
    ;잘못입력
        INPUT
        CLEARLINE 1
    ENDIF
WEND


;---------------------------------------
;@SET_TALENT, CHARA_NUM
;TALENT 설정하는 함수
;---------------------------------------
@SET_TALENT, CHARA_NUM
#DIM DYNAMIC CHARA_NUM
;캐릭터ID
#DIM DYNAMIC STORE_TARGET
;타겟 저장
#DIM DYNAMIC DICE,3
;주사위

STORE_TARGET = TARGET
;타겟 저장
TARGET = CHARA_NUM

;성격 - 21~25
;성격1
DICE = 21 + RAND:5
IF RAND:2 == 0
    TALENT:DICE = -1
ELSE
    TALENT:DICE = 1
ENDIF
;성격2 - 50% 안나올수도 있음. 5%확률로 그대로 거나, 5%확률로 반전됨.
DICE = 21 + RAND:10
IF DICE <= 21 && DICE < 26
    IF RAND:2 == 0
        TALENT:DICE = -1
    ELSE
        TALENT:DICE = 1
    ENDIF
ENDIF

;성애의 관심 - 51~58
REPEAT 8
    DICE = RAND:10
    IF DICE <= 0 && DICE < 3
        IF (COUNT == 0 || COUNT == 1) && RAND:2 == 0
            TALENT:(COUNT+51) = -1
        ELSE
            TALENT:(COUNT+51) = 1
        ENDIF
    ENDIF
REND

;소녀 감성 - 61~64
REPEAT 4
    DICE = RAND:10
    IF DICE <= 0 && DICE < 3
        IF (COUNT == 0 || COUNT == 1 || COUNT == 3) && RAND:2 == 0
            TALENT:(COUNT+61) = -1
        ELSE
            TALENT:(COUNT+61) = 1
        ENDIF
    ENDIF
REND

;체질 - 71~74
REPEAT 4
    DICE = RAND:10
    IF DICE <= 0 && DICE < 2
        IF (COUNT == 0 || COUNT == 1 || COUNT == 2) && RAND:2 == 0
            TALENT:(COUNT+71) = -1
        ELSE
            TALENT:(COUNT+71) = 1
        ENDIF
    ENDIF
REND

;기술 - 81~83
REPEAT 3
    DICE = RAND:10
    IF DICE <= 0 && DICE < 2
        IF (COUNT == 0) && RAND:2 == 0
            TALENT:(COUNT+81) = -1
        ELSE
            TALENT:(COUNT+81) = 1
        ENDIF
    ENDIF
REND

;결벽도 - 91~93
REPEAT 3
    DICE = RAND:10
    IF DICE <= 0 && DICE < 2
        IF COUNT == 1
        ;92,악취내성
            DICE:1 = RAND:12
            IF DICE:1 >= 0 && DICE:1 < 2
                TALENT:(COUNT+91) = -2
            ELSEIF DICE:1 >= 2 && DICE:1 < 6
                TALENT:(COUNT+91) = -1
            ELSEIF DICE:1 >= 6 && DICE:1 < 10
                TALENT:(COUNT+91) = 1
            ELSE
                TALENT:(COUNT+91) = 2
			ENDIF
        ELSE
            TALENT:(COUNT+91) = 1
        ENDIF
    ENDIF
REND

;정직도A - 101~102
REPEAT 2
    DICE = RAND:10
    IF DICE <= 0 && DICE < 1
        IF (COUNT == 0) && RAND:2 == 0
            TALENT:(COUNT+101) = -1
        ELSE
            TALENT:(COUNT+101) = 1
        ENDIF
    ENDIF
REND

;정직도B - 104~108 (1개 제한)
DICE = RAND:20
IF DICE <= 0 && DICE < 1
    TALENT:((RAND:5)+104) = 1
ENDIF

;성애성 - 111~118 (2개 제한)
REPEAT 2
    DICE = RAND:10
    IF DICE <= 0 && DICE < 1
        DICE:1 = RAND:8
        IF DICE:1 == 1
        ;112,성별기호
            DICE:1 = RAND:3
            IF DICE:1 == 0
                TALENT:(DICE:1+111) = -1
            ELSE
                ;DICE:1 == 1 || 2
                TALENT:(DICE:1+111) = DICE:1
            ENDIF
        ELSE
            TALENT:(DICE:1+111) = 1
        ENDIF
    ENDIF
REND

;매료 - 121~123
REPEAT 3
    DICE = RAND:20
    IF DICE <= 0 && DICE < 1
        TALENT:(COUNT+121) = 1
    ENDIF
REND

;몸 특징A - 131~132
;131,체형,;-3=난쟁이 -2=유아 체형 -1=작은 체형 1=장신 2=거구 3=거인
;132,가슴사이즈,;-2=절벽 -1 = 작은 가슴 0=미유 1=거유 2=폭유 3=초유
REPEAT 2
    IF RAND:3 == 0 && COUNT == 0
    ;66퍼 확률로 평범
    ;체형 일때, -3:난쟁이, 3:거인 -> 종족특수
    ;1, 3, 3, 1
        DICE = RAND:8
        IF DICE == 0
            TALENT:(COUNT+131) = -2
        ELSEIF DICE >= 1 && DICE < 4
            TALENT:(COUNT+131) = -1
        ELSEIF DICE >= 4 && DICE < 7
            TALENT:(COUNT+131) = 1
        ELSE
            TALENT:(COUNT+131) = 2
        ENDIF
    ENDIF
	IF RAND:3 == 0 && COUNT == 1
        ;가슴사이즈 일때 66퍼 확률로 평범.
        ;1, 4, 4, 2, 1
        DICE = RAND:12
        IF DICE == 0
            TALENT:(COUNT+131) = -2
        ELSEIF DICE >= 1 && DICE < 5
            TALENT:(COUNT+131) = -1
        ELSEIF DICE >= 5 && DICE < 9
            TALENT:(COUNT+131) = 1
        ELSEIF DICE >= 9 && DICE < 11
            TALENT:(COUNT+131) = 2
        ELSE
            TALENT:(COUNT+131) = 3
        ENDIF
    ENDIF
REND

;몸특징B - 133~137(3개 제한)
REPEAT 3
    DICE = RAND:10
    IF DICE <= 0 && DICE < 4
        IF RAND:3 == 0
            TALENT:((RAND:5)+133) = -1
        ELSE
            TALENT:((RAND:5)+133) = 1
        ENDIF
    ENDIF
REND

;몸특징C - 138
DICE = RAND:10
IF DICE <= 0 && DICE < 3
    TALENT:(COUNT+138) = 1
ENDIF

;몸특징D - 140,141
DICE = RAND:12
IF DICE == 0
	TALENT:140 = 1
	ABL:91 += 5
	ABL:92 += 5
	;근육질
ELSEIF DICE == 1
	TALENT:141 = 1
	ABL:91 -= 5
	ABL:92 -= 5
